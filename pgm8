public class Program8 
{
    public static void main(String[] args)
    {
        int age=62;
        if(age<12)
    {
        System.out.println("Minor");
    }
        else if(age>=12 && age<60)
    {
        System.out.println("Normal");
    }
        else if(age>60)
    {
        System.out.println("sinior citizen");
            
    }
            
        
                
    }
    
}
