package day2programs;
public class MaxEleImdex {
    public static void main(String[] args) {
        int[] arr = {5, 10, 20, 8, 15};
        
        int max = arr[0];
        int maxIndex = 0;

        for (int i = 1; i < arr.length; i++) {
            if (arr[i] > max) {
                max = arr[i];
                maxIndex = i;
            }
        }

        System.out.println("Maximum element is at index: " + maxIndex);
    }
}
