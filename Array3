package Arrays;
public class Array3 {
    public static void main(String[] args){
        double[]  height = {3.4,5.5,6.2,2.7};
        System.out.println(height[2]);
        System.out.println(height[3]);
        System.out.println(height[1]);
        System.out.println(height[4]);
        
        
    }
    
}
