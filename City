package methodoverloding;
public class City {
    void place(int a){
        System.out.println("Udupi");
    }
     void place(int a,int b){
        System.out.println("Bantakal");
     }
      void place(int a,int b,int c){
        System.out.println("Mongalore");
      }
      public static void main(String[] args){
          City c=new City();
          c.place(10);
          c.place(10,20);
          c.place(10,20,30);
      }
}
    
