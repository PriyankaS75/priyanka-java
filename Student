package Module2;
public class Student {
    int age =20;
    String name = "Vani";
    
    void read()
    {
        System.out.println("Vani is reading");    
    }
    public static void main(String[] args){
        Student s=new Student();
        System.out.println(s.age);
        System.out.println(s.name);
        s.read();
  }
    
}
